    "SpinupVariables": {
        "delay": 0,
        "run_delay": false,
        "minimum_rotation": 10,
        "spinup_parameters": {
            "transform": {
                "library": "internal.flint",
                "type": "SQLQueryTransform",
                "provider": "SQLite",
