        "MossDisturbanceModule": {
            "library": "moja.modules.cbm",
            "order": 14,
            "create_new": true
        },
        "CBMSpinupDisturbanceModule": {
            "library": "moja.modules.cbm",
            "order": 15,
            "create_new": true
